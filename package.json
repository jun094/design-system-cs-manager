{
  "name": "design-system-cs-helper",
  "version": "1.0.0",
  "description": "디자인 시스템 라이브러리 CS 관리를 위한 크롬 확장 프로그램",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "watch": "vite build --watch",
    "build:ext": "cross-env VITE_IS_EXTENSION=true tsc && vite build",
    "build:no-typecheck": "vite build",
    "build:ignore-errors": "tsc --noEmit false --skipLibCheck true || vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.253",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  }
} 